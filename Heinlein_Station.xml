    <routines>
      <routine name="not_possible">if (dobj == player)
{
     println("Not physically possible.");
} </routine>
      <routine name="get_portable">if (dobj.portable == 1) { if (dobj.holder != player) { println("(Taken)"); dobj.holder = player;}  } </routine>
      <routine name="kill_self">println("If you are experiencing suicidal thoughts you should seek psychiatric help.");</routine>
      <routine name="default_kill">println("Perhaps you should count to 3 and calm down.");</routine>
      <routine name="kill_player">println("***YOU HAVE DIED***.");player.holder=2;
</routine>
      <routine name="talk_to_self">println("Talking to yourself is a sign of impending mental collapse.");</routine>
      <routine name="default_talk">println("That fails to produce an exciting conversation.");</routine>
      <routine name="listen">println("You hear nothing unexpected.");</routine>
      <routine name="smell">println("You smell nothing unexpected.");</routine>
      <routine name="wait">println("Time passes...");</routine>
      <routine name="yell">println("AAAAAAAAAAAAARRRRGGGGGG!");</routine>
      <routine name="jump">println("WHEEEEEE!");
</routine>
      <routine name="default_eat">println("That's not part of a healthy diet.");
</routine>
      <routine name="default_drink">println("That's not something you can drink.");
</routine>
      <routine name="go_up">println("The turbo lift rises to the top level of the station.");
println("The door to the west opens, revealing a command center.");
player.holder = turbo_lift_2;
</routine>
      <routine name="go_down">println("The turbo lift rises to the ground level of the station.");
println("The door to the west opens, revealing a corridor.");
player.holder = turbo_lift_1;
</routine>
      <routine name="use_airlock1">if (airlock1Open == 0)
{
     println("A computerized voice says, 'WARNING: DEPRESSSURIZING AIRLOCK'");
     airlock1Open = 1;
     airlock_1.w = lunar_surface;
     airlock_1.e = 255;
}
else
{
     println("A computerized voice says, 'RESTORING ATMOSPHERIC PRESSURE'");
     airlock1Open = 0;
     airlock_1.w = 255;
     airlock_1.e = 2;
}
</routine>
    </routines>
